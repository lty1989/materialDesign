<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.materialdesign">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ToolbatActivity" />
        <activity android:name=".DrawerActivity" />
        <activity android:name=".NavigationActivity" />
        <activity android:name=".FloatingActionActivity" />
        <activity android:name=".CoordinatorActivity" />
        <activity android:name=".AppBarActivity" />
        <activity
            android:name=".CollapsingToobarActivity"
            android:theme="@style/CollaspingTheme" />

    </application>

</manifest>